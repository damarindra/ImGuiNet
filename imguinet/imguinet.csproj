      <HintPath>..\CppSharp-0.10.4-vs2017-x64\CppSharp.AST.dll</HintPath>
    </Reference>
    <Reference Include="CppSharp.Generator">
      <HintPath>..\CppSharp-0.10.4-vs2017-x64\CppSharp.Generator.dll</HintPath>
    </Reference>
    <Reference Include="CppSharp.Parser">
      <HintPath>..\CppSharp-0.10.4-vs2017-x64\CppSharp.Parser.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
